{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "standard src/**/*.js --verbose && standard src/*.js --verbose",
    "clean": "rimraf dist && mkdirp dist",
    "browserify:index": "browserify src/index.js -t babelify -d -o dist/bundle.js",
    "browserify:clients": "browserify src/clients.js -t babelify -d -o dist/bundle-clients.js",
    "browserify:vod": "browserify src/vod.js -t babelify -d -o dist/bundle-vod.js",
    "build": "npm run lint && npm run clean && parallelshell \"npm run browserify:index\" \"npm run browserify:clients\" \"npm run browserify:vod\""
  },
