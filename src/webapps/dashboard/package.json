    "isomorphic-fetch": "^2.2.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.4",
    "standard": "^7.1.2"
  },
  "dependencies": {
    "chart.js": "^2.2.1",
    "moment": "^2.14.1",
    "video.js": "^5.10.8",
